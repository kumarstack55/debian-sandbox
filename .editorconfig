# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = false
max_line_length = unset
tab_width = 2
trim_trailing_whitespace = true

[Vagrantfile]

[*.md]
indent_size = 4
tab_width = 4

[*.sh]
# https://google.github.io/styleguide/shell.xml#Formatting
end_of_line = lf
